{
     "printWidth": 400,
     "trailingComma": "none",
     "tabWidth": 5,
     "semi": true,
     "singleQuote": true,
     "bracketSpacing": true,
     "arrowParens": "always",
     "endOfLine": "auto",
     "jsxSingleQuote": true,
